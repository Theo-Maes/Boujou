{
  "name": "boujou-normandie",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:test": "env-cmd -f .env.test.local npx prisma migrate reset && export NODE_ENV=\"test\" && next build",
    "start": "next start",
    "start:test": "export NODE_ENV=\"test\" && next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "test": "env-cmd -f .env.test.local vitest",
    "cypress:open": "env-cmd -f .env.test.local cypress open",
    "cypress:start": "start-server-and-test 'npm run start:test' 3000 'npm run cypress:open'",
    "cypress:build": "npm run build:test && npm run cypress:start",
    "cypress:run": "start-server-and-test 'npm run start:test' 3000 'env-cmd -f .env.test.local cypress run'",
    "e2e:run": "npx prisma migrate reset && start-server-and-test 'npm run start:test' 3000 'cypress run'"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/core": "^0.31.0",
    "@auth/prisma-adapter": "^2.0.0",
    "@aws-sdk/client-s3": "^3.592.0",
    "@aws-sdk/s3-request-presigner": "^3.592.0",
    "@ducanh2912/next-pwa": "^10.2.7",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@headlessui/react": "^2.0.3",
    "@hookform/resolvers": "^3.6.0",
    "@mui/material": "^5.15.20",
    "@mui/x-date-pickers": "^7.7.0",
    "@nextui-org/react": "^2.4.2",
    "@prisma/client": "^5.13.0",
    "@react-aria/i18n": "^3.11.1",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.3",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "framer-motion": "^11.2.12",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-aria": "^3.33.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "react-icons": "^5.2.1",
    "sharp": "^0.33.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.13.1",
    "cypress-file-upload": "^5.0.8",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "^14.2.3",
    "jsdom": "^24.1.1",
    "msw": "^2.3.2",
    "next-test-api-route-handler": "^4.0.8",
    "postcss": "^8.4.38",
    "prisma": "^5.13.0",
    "start-server-and-test": "^2.0.4",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^2.0.3"
  }
}
